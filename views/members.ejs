<div class="body flex-grow-1">
  <div class="container-lg px-4">
      <table class="table table-hover">
          <thead>
            <tr>
              <th scope="col">#</th>
              <th scope="col">Title</th>
              <th scope="col">Farm Name</th>
              <th scope="col">Status</th>
              <th scope="col">Action</th>
            </tr>
          </thead>
          <tbody>
            <% for(var i=0; i < members.length; i++) { %>
            <tr>
              <th scope="row"><%= [i+1] %></th>
              <td><%= members[i].owner_name %></td>
              <td><%= members[i].farm_name %></td>
              <td><%= members[i].user_level %></td>
              <td>
                  <a href="/edit-member/<%= members[i]._id %>" type="button" class="btn btn-success btn-sm  text-white">Edit</a>
                  <a href="#" type="button" class="btn btn-danger btn-sm text-white">Delete</a>
              </td>
            </tr>
            <% } %>
          </tbody>
        </table>
  </div>
</div>